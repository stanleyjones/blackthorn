    "express": "^4.13.3",
    "express-graphql": "^0.5.1",
    "express-session": "^1.11.3",
    "graphql": "^0.5.0",
    "jsonwebtoken": "^7.0.0",
    "mongoose": "^4.1.6",
    "nodemailer": "^1.4.0",
    "nodemon": "^1.9.2",
    "passwordless": "^1.1.0",
    "passwordless-mongostore": "^0.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
