{
  "name": "blackthorn",
  "description": "A gaming dashboard for GURPS.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "seed": "babel-node ./bin/seed_database",
    "start": "npm run start:client & npm run start:server",
    "start:client": "if test \"$NODE_ENV\" != \"production\"; then react-scripts start; fi",
    "start:server": "nodemon server --exec babel-node",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "body-parser": "^1.15.1",
    "cors": "^2.8.1",
    "express": "^4.13.3",
    "express-graphql": "^0.6.0",
    "graphql": "^0.9.1",
    "jsonwebtoken": "^7.0.0",
    "mongodb": "^2.1.19",
    "nodemailer": "^3.1.3",
    "nodemon": "^1.9.2",
  },
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-scripts": "^0.9.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "no-console": 0
    }
  }
}
